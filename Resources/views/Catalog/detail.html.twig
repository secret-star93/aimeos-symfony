{% extends '@AimeosShop/base.html.twig' %}

{% block aimeos_scripts %}
{{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/aimeosshop/themes/aimeos-detail.js') }}"></script>
{% endblock %}

{% block aimeos_header %}
    {{ (aiheader['basket/mini'] ?? '')|raw }}
    {{ (aiheader['catalog/stage'] ?? '')|raw }}
    {{ (aiheader['catalog/detail'] ?? '')|raw }}
    {{ (aiheader['catalog/session'] ?? '')|raw }}
{% endblock %}

{% block aimeos_head %}
    {{ (aibody['basket/mini'] ?? '')|raw }}
{% endblock %}

{% block aimeos_stage %}
    {{ (aibody['catalog/stage'] ?? '')|raw }}
{% endblock %}

{% block aimeos_body %}
    {{ (aibody['catalog/detail'] ?? '')|raw }}
{% endblock %}

{% block aimeos_aside %}
    {{ (aibody['catalog/session'] ?? '')|raw }}
{% endblock %}
