 {% extends '::base.html.twig' %}

 {% block aimeos_styles %}
    {% stylesheets
        'bundles/aimeosshop/themes/elegance/common.css'
        'bundles/aimeosshop/themes/elegance/aimeos.css'
        filter='cssrewrite'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
 {% endblock %}

 {% block aimeos_scripts %}
    {% javascripts
        'bundles/aimeosshop/themes/jquery-ui.custom.min.js'
        'bundles/aimeosshop/themes/aimeos.js'
        'bundles/aimeosshop/themes/elegance/aimeos.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
 {% endblock %}
