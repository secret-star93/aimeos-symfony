{
	"name": "aimeos/aimeos-symfony2",
	"description": "Aimeos Symfony2 web shop bundle",
	"homepage": "http://aimeos.org",
	"type": "symfony-bundle",
	"license": "MIT",
	"keywords": ["aimeos", "symfony", "e-commerce", "shop", "portal", "market place"],
	"support": {
		"source": "https://github.com/Aimeos/symfony-bundle",
		"issues": "https://github.com/Aimeos/symfony-bundle/issues",
		"forum": "http://help.aimeos.org",
		"wiki": "http://docs.aimeos.org"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/arcavias-core"
		}
	],
    "minimum-stability": "dev",
	"require": {
		"php": ">=5.3.3",
		"arcavias/arcavias-core": "dev-master",
		"aimeos/extinstaller": "dev-master",
		"aimeos/ai-symfony2": "dev-master",
		"aimeos/ai-monolog": "dev-master",
		"aimeos/ai-zend2-i18n": "dev-master",
		"aimeos/ai-swiftmailer": "dev-master",
		"symfony/symfony": "2.5.*"
	},
	"require-dev": {
		"doctrine/orm": "~2.2,>=2.2.3",
		"doctrine/doctrine-bundle": "~1.2",
		"symfony/monolog-bundle": "~2.3",
		"symfony/swiftmailer-bundle": "~2.3",
		"sensio/framework-extra-bundle": "~2.3",
		"symfony/assetic-bundle": "~2.3",
		"symfony/twig-bundle": "~2.1",
		"twig/extensions": "~1.0"
	},
	"autoload": {
		"psr-4": { "Aimeos\\ShopBundle\\": "" }
	}
}
